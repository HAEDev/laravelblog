{
    "name": "lnch/laravelblog",
    "description": "A complete Blog module, with category, tag, and images",
    "authors": [
        {
            "name": "Tom Lynch",
            "email": "tom@lnch.co.uk"
        }
    ],
    "licence": "MIT",
    "minimum-stability": "dev",
    "require": {
        "doctrine/dbal": "*"
    },
    "autoload": {
        "psr-4": {
            "Lnch\\LaravelBlog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lnch\\LaravelBlog\\LaravelBlogServiceProvider"
            ],
			"aliases": {
				"LaravelBlog": "Lnch\\LaravelBlog\\LaravelBlogFacade"
			}
        }
    }
}
